{
    "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.12.0/.schema/devbox.schema.json",
    "packages": [
      "kind@0.23.0",
      "gh@2.50.0",
      "jq@1.7.1",
      "yq-go@4.44.1",
      "kubectl@1.31.2",
      "k9s@0.31.9",
      "tmux@3.4",
      "bat@latest",
      "zoxide@latest",
      "fzf@latest",
      "eza@latest",
      "starship@1.20.1"
    ],
    "shell": {
      "init_hook": [
        "autoload -Uz compinit",
        "compinit",
        "source <(kubectl completion zsh)"
      ],
      "scripts": {}
    }
  }